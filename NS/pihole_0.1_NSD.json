{
  "name":"Pihole NSD",
  "vendor":"Genicom",
  "version":"0.1",
  "vld":[
    {
      "name":"shared_admin"
    }
  ],
  "vnfd":[
    {
      "name":"pihole-server",
      "vendor":"Genicom",
      "version":"0.1",
      "lifecycle_event":[
        {
          "event":"INSTANTIATE",
          "lifecycle_events":[
            "scripts/pihole_install.sh",
            "scripts/pihole_config.sh"
          ]
        }
      ],
      "virtual_link":[
        {
          "name":"shared_admin"
        }
      ],
      "vdu":[
        {
          "vm_image":[
            "Trusty2"
          ],
          "scale_in_out":1,
          "vnfc":[
            {
              "connection_point":[
                {
                  "virtual_link_reference":"shared_admin",
                  "interfaceId":0
                }
              ]
            }
          ]
        }
      ],
      "deployment_flavour":[
        {
          "flavour_key":"m1.small"
        }
      ],
      "type":"server",
      "endpoint":"generic",
      "vnfPackageLocation":"https://github.com/faucherjacques/openbaton.git"
    }
  ]
}