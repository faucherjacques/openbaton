{
  "name":"pihole",
  "vendor":"Genicom",
  "version":"0.1",
  "lifecycle_event":[
	{
	  "event":"CONFIGURE",
	  "lifecycle_events":[
		"pihole_config.sh"
	  ]
	},
	{
	  "event":"INSTANTIATE",
	  "lifecycle_events":[
		"pihole_install.sh"
	  ]
	}
  ],
  "vdu":[
	{
	  "vm_image":[
		"xenial"
	  ],
	  "scale_in_out":1,
	  "vnfc":[
		{
		  "connection_point":[
			{
			  "virtual_link_reference":"pihole_net",
			  "interfaceId":0
			}
		  ]
		}
	  ]
	}
  ],
  "virtual_link":[
	{
	  "name":"pihole_net"
	}
  ],
  "deployment_flavour":[
	{
	  "flavour_key":"m1.small"
	}
  ],
  "type":"client",
  "endpoint":"generic",
  "vnfPackageLocation":"https://github.com/faucherjacques/openbaton/scripts.git",
}